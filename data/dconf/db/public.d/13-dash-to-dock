[org/gnome/shell/extensions/dash-to-dock]
# Enable or disable autohide mode
autohide=false
# Enable or disable autohide mode
autohide-in-fullscreen=false
# Dock is always visible
dock-fixed=true
# The side of the screen that the dock is shown [Left 'LEFT' | Top 'TOP' | Right 'RIGHT' | Bottom 'BOTTOM']
dock-position='BOTTOM'
# Extend the dock container to all the available height/width
extend-height=true
# Enable od disable-intellihide mode
intellihide=false
# The intellhide mode ['ALL_WINDOWS' | 'FOCUS_APPLICATION_WINDOWS' | 'MAXIMIZED_WINDOWS' | 'ALWAYS_ON_TOP']
intellihide-mode='ALWAYS_ON_TOP'
# Enable or disable manualhide mode
manualhide=false
# Show a dock on every monitor
multi-monitor=true
# Show or hide mounted volume and device icons in the dash
show-mounts=false
# Show ow hide network volumes in the dash
show-mounts-network=false
# Show or hide unmounted volume and device icons in the dash
show-mounts-only-mounted=true
# Show or hide running applications icons in the dash
show-running=true
# Show applications button in the dash
show-show-apps-button=true
# Show or hide the trash can icon in the dash
show-trash=false